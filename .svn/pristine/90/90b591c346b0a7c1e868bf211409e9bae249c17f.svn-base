<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from SecretsUpdate WSDL service -->

  <service name="SecretsUpdateHttpPort" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="webservices.secretupdate.weaver.com.cn"/>
      <parameter name="wsdlServiceElement" value="SecretsUpdate"/>
      <parameter name="schemaQualified" value="webservices.secretupdate.weaver.com.cn"/>
      <parameter name="wsdlServicePort" value="SecretsUpdateHttpPort"/>
      <parameter name="className" value="kd.cus.wb.FWsecrets.SecretsUpdateHttpBindingSkeleton"/>
      <parameter name="wsdlPortType" value="SecretsUpdatePortType"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>
      <parameter name="scope" value="Session"/>

  </service>
</deployment>
